<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated query -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="query.presentation.QueryModelWizardID"
            name="%_UI_QueryModelWizard_label"
            class="query.presentation.QueryModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/QueryModelFile.gif">
         <description>%_UI_QueryModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated query -->
      <editor
            id="query.presentation.QueryEditorID"
            name="%_UI_QueryEditor_label"
            icon="icons/full/obj16/QueryModelFile.gif"
            extensions="query"
            class="query.presentation.QueryEditor"
            contributorClass="query.presentation.QueryActionBarContributor">
      </editor>
   </extension>

</plugin>
